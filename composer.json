{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "caouecs/laravel-lang": "~4.0",
        "codedge/laravel-selfupdater": "^2.5",
        "delight-im/random": "^1.0",
        "fideloper/proxy": "^4.0",
        "gecche/laravel-multidomain": "2.*",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "madnest/madzipper": "^1.0",
        "phpoffice/phpword": "^0.17.0",
        "ramsey/uuid": "^3.9",
        "rap2hpoutre/fast-excel": "^1.7",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^3.3",
        "yajra/laravel-datatables-oracle": "^9.8"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database",
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "database/seeds/UsersAndNotesSeeder.php",
            "database/seeds/MenusTableSeeder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
